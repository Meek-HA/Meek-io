post-up iptables -A PREROUTING -t nat -i vmbr0 -p tcp --dport 10402 -j DNAT --to \10.10.10.104
post-up iptables -A POSTROUTING -t nat -o vmbr1 -p tcp --dport 10402 -d 10.10.10.104 -j SNAT --to \185.252.233.184
post-up iptables -A PREROUTING -t nat -i vmbr0 -p tcp --dport 10501 -j DNAT --to \10.10.10.105
post-up iptables -A POSTROUTING -t nat -o vmbr1 -p tcp --dport 10501 -d 10.10.10.105 -j SNAT --to \185.252.233.184
